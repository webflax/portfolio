{
    "name": "webflaxco/portfolio",
    "description": "portfolio package for laravel",
    "homepage": "https://github.com/webflax/portfolio",
    "type": "library",
    "keywords": [
        "portfolio",
        "freelancer",
        "project"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "webflaxco",
            "email": "webflax.developers@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Webflax\\Portfolio\\App\\Providers\\PortfolioServiceProvider\\"
            ],
            "aliases": {
                "Portfolio": "Webflax\\Portfolio\\services\\Facades\\Portfolio\\"
            }
        }
    },
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/pagination": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "cviebrock/eloquent-sluggable": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.27"
    },
    "autoload": {
        "psr-4": {
            "Webflax\\Portfolio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webflax\\Portfolio\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
